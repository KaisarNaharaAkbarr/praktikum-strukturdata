# Implementasi Queue versi baru
queue = list()

# Menambah elemen ke antrian (enqueue)
for item in ['A', 'B', 'C']:
    queue.append(item)
print("Isi queue saat ini :", queue)

# Menghapus elemen pertama (dequeue)
if queue:
    removed_item = queue.pop(0)
    print("Elemen yang di-dequeue :", removed_item)

# Melihat elemen depan tanpa menghapus (peek)
if queue:
    first_item = queue[0]
    print("Elemen paling depan sekarang :", first_item)

# Mengecek apakah queue kosong
queue_empty = len(queue) == 0
print("Queue kosong? :", queue_empty)

# Menampilkan total elemen
print("Jumlah elemen di queue :", len(queue))
